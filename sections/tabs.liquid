
{% stylesheet %}
    .llytab-box{
    padding:20px 0;
    }

.showPlace{
width:100%;
height:50vh;
background-color:#ccc;

}
.showPlace div{
    display:none;
}

.showPlace .fs{
    display:block;
}
.llytab{
    display:flex;
    justify-content: center;
    align-items: center;
    padding:0 ;
    margin:0;
    margin-top:20px;
}
.llytab  li{
    list-style: none;
 
    padding: 5px 10px;
}
.llytab  li.cur {
	background:blue;
} 

{% endstylesheet %}

<div class="llytab-box ">

    <div class="page-width">

    <div class="showPlace">
        <div class="fs"></div>
        <div >22</div>
        <div>33</div>
    </div>



    <div>
        <ul class="llytab"> 
           {% for block in section.blocks %}
            {% case block.type %}
                {%- when 'title' -%}
                    {%- if block.settings.title != blank -%}
                        <li class="dddddd">
                            {% render 'icon-accordion', icon: block.settings.icon %}{{block.settings.title}}
                        </li>
                        {%- endif -%}
                
                    



            {% endcase %}



           {% endfor %}
        </ul>
    </div>

    

</div>
</div>




